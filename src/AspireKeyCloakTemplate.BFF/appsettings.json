{
  "ClientId": "sandbox-gateway",
  "ClientSecret": "supersecret",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "api": {
        "ClusterId": "api",
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPrefix": "/api"
          }
        ]
      },
      "react-app": {
        "ClusterId": "react-app",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "react-app": {
        "Destinations": {
          "react-app": {
            "Address": "http://react-app"
          }
        }
      },
      "api": {
        "Destinations": {
          "api/destination1": {
            "Address": "https://api"
          }
        }
      }
    }
  }
}
