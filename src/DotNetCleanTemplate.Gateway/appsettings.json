{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "react": {
        "ClusterId": "react",
        "Match": {
          "Path": "/{**catch-all}"
        }
      },
      "api": {
        "ClusterId": "api",
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPrefix": "/api"
          }
        ]
      }
    },
    "Clusters": {
      "react": {
        "Destinations": {
          "react/destination1": {
            "Address": "http://localhost:51178"
          }
        }
      },
      "api": {
        "Destinations": {
          "api/destination1": {
            "Address": "https://dotnetcleantemplate-api"
          }
        }
      }
    }
  }
}
